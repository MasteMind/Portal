
    <%= form_for @student do |f| %>
      <%= f.text_field :name, :placeholder => "Name" %>
      <%= f.email_field :email, :placeholder => "Email" %>
      <%= f.text_field :contact, :placeholder => "Contact" %>
      <hr>
      <%= f.submit "Create an account", class: "btn btn-primary" %>
    <% end %> 
